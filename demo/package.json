{
  "name": "@articulate/sox-demo",
  "version": "0.0.0",
  "description": "Demo app for articulate/sox",
  "main": "server/index.js",
  "repository": "git@github.com:articulate/sox.git",
  "author": "articulate",
  "license": "MIT",
  "scripts": {
    "build:styles": "lessc client/index.less | postcss -u autoprefixer | cleancss -O2 --inline all > dist/index.css",
    "clean": "rm -rf dist && mkdir -p dist",
    "lint": "eslint client server",
    "prestart": "yarn run clean",
    "prestart:styles": "yarn run build:styles",
    "start": "run-p start:*",
    "start:scripts": "watchify client/index.js -dv -o 'exorcist dist/index.js.map > dist/index.js'",
    "start:serve": "nodemon -w server .",
    "start:styles": "chokidar 'client/styles/*.less' -c 'yarn run build:styles' --silent"
  },
  "dependencies": {
    "@articulate/funky": "^1.3.0",
    "crocks": "^0.10.1",
    "dampen": "^0.0.1",
    "joi": "^13.6.0",
    "level": "^4.0.0",
    "moment": "^2.22.2",
    "paperplane": "^2.0.0",
    "puddles": "^2.1.6",
    "pug": "^2.0.3",
    "ramda": "^0.25.0",
    "redux-functor": "^1.0.0",
    "redux-future2": "^1.0.0",
    "require-dir": "^1.0.0",
    "socket.io": "^2.1.1",
    "tinyfunk": "^1.2.1",
    "tinygen": "^0.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.1.3",
    "chokidar-cli": "^1.2.0",
    "clean-css-cli": "^4.2.1",
    "eslint": "^5.4.0",
    "exorcist": "^1.0.1",
    "less": "^3.8.1",
    "nodemon": "^1.18.3",
    "normalize.css": "^8.0.0",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^6.0.0",
    "watchify": "^3.11.0"
  }
}
